{
  "name": "jns42-generator",
  "version": "0.0.0",
  "description": "generate interfaces and validators from a json schema",
  "type": "module",
  "bin": "out/program.js",
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "out/*",
    "src/*"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "clean": "rm -rf out",
    "test": "npm run spec-all",
    "lint": "eslint \"src/**/*.ts\"",
    "spec-all": "node --test ./out/**/*.spec.js",
    "coverage": "c8 --reporter lcov npm test",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:JsonSchema42/jns42-generator-ts"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JsonSchema42/jns42-generator-ts/issues"
  },
  "keywords": [
    "json",
    "schema",
    "generator"
  ],
  "homepage": "https://www.JsonSchema42.org",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "c8": "^7.13.0",
    "eslint": "^8.40.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-security": "^1.7.1",
    "typedoc": "^0.23.28"
  },
  "dependencies": {
    "@types/node": "^18.16.13",
    "@types/yargs": "^17.0.24",
    "camelcase": "^7.0.1",
    "crc": "^4.3.2",
    "tslib": "^2.5.2",
    "type-fest": "^3.10.0",
    "typescript": "^4.9.5",
    "yargs": "^17.7.2"
  }
}
