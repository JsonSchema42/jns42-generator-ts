{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://JsonSchema42.org/simple-object",
  "description": "A very simple object",
  "type": [
    "object"
  ],
  "required": [
    "id"
  ],
  "properties": {
    "id": {
      "description": "An identity",
      "type": "integer",
      "exclusiveMinimum": 0
    },
    "name": {
      "description": "A name",
      "type": "string",
      "minLength": 2,
      "maxLength": 20
    }
  }
}
