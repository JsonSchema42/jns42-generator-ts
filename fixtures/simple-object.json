{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://goodschema.org/simple-object",
  "type": [
    "object"
  ],
  "required": [
    "id"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "exclusiveMinimum": 0
    },
    "name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 20
    }
  }
}